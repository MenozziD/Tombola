<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:id="@+id/bordo_sinistro">
        <shape android:shape="rectangle">

            <padding
                android:left="10dp"
                android:top="10dp"/>

            <solid android:color="#000000" />

        </shape>
    </item>
    <item
        android:id="@+id/bordo_destro">
        <shape
            android:shape="rectangle">
            <padding
                android:right="5dp"
                android:bottom="5dp"/>

            <solid android:color="#000000" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle">

            <solid
                android:color="#FFFFFF"/>

            <size
                android:height="85dp"
                android:width="85dp"/>
        </shape>
    </item>
</layer-list>